import java.lang.*;
import java.util.*;
class str
{
	public static void main(String args[])
	{
		String name1="Reena";
		System.out.println(name1);
		String name2=name1.concat("Meena");
		System.out.println(name2);
		StringBuilder sb=new StringBuilder(name2);
		System.out.println(sb.reverse());
	}
}
